<%= stylesheet_link_tag "miscellaneous" %>
<%= javascript_include_tag "miscellaneous" %>
<%= stylesheet_link_tag "commerce" %>
<%= javascript_include_tag "commerce" %>

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-8">
        <h2>Manage Organization: <%= @organization.name %></h2>
    </div>
    <div class="col-sm-4">
        <h2>ID: <%= @organization.id %></h2>
    </div>
</div>

<div class="row justify-content-md-center">
    <% flash.each do |key, value| %>
        <% if key == "success" || key == "notice" %>
            <div class="col-lg-7 alert alert-dismissable alert-success">
            <button aria-hidden="true" data-dismiss="alert" class="close" type="button">×</button>
        <% else %>
            <div class="col-lg-7 alert alert-dismissable alert-danger">
            <button aria-hidden="true" data-dismiss="alert" class="close" type="button">×</button>
        <% end %>
            <%= value %>
        </div>
    <% end %>
</div>

<div class="wrapper wrapper-content animated fadeInUp">
    <!-- Nav Items -->
    <div class="row text-center">
        <div class="col-lg-12">
            <h3>Jump To</h3>
        </div>
        <div class="col-lg-12">
            <a href="#org-details" class="btn btn-warning">Org Details</a>

            <a href="#org-phone" class="btn btn-warning">Phone Details</a>

            <a href="#org-plan" class="btn btn-warning">Plan Details</a>

            <a href="#org-billing" class="btn btn-warning">Billing Details</a>

            <a href="#org-stripe" class="btn btn-warning">Stripe Details</a>

            <a href="#org-notes" class="btn btn-warning">Notes</a>

            <a href="#org-records" class="btn btn-warning">Records</a>

        </div>
    </div>
    <br />
    <hr />
    <br />
    <!-- Org Details -->
    <div class="row" id="org-details">
        <div class="col-lg-12">
            <div class="ibox">
                <div class="ibox-content">
                    <div class="row">
                        <div class="col-lg-12 text-center">
                            <h3>Organization Details</h3>
                            <small>All times and date are shown in the organization's time zone.</small>
                            <hr />
                        </div>
                    </div>
                    <div class="row">
                        <!-- Logo -->
                        <div class="col-lg-6">
                            <dl class="row mb-0">
                                <div class="col-sm-4 text-sm-right">
                                    <dt>Logo:</dt>
                                </div>
                                <div class="col-sm-8 text-sm-left">
                                    <% if !@organization.logo.blank? %>
                                        <dd class="mb-1"><img id="img_prev" width=100 height=100 src="<%= @organization.logo.url %>" alt="your image" class="img-thumbnail float-left"/></dd>
                                    <% else %>
                                        <dd class="mb-1">No Logo Provided</dd>
                                    <% end %>
                                </div>
                            </dl>
                        </div>

                        <!-- Status -->
                        <div class="col-lg-6">
                            <dl class="row mb-0">
                                <div class="col-sm-4 text-sm-right">
                                    <dt>Status:</dt>
                                </div>
                                <div class="col-sm-8 text-sm-left">
                                    <% if @organization.active %>
                                        <dd class="mb-1"><span class="label label-primary">Active</span></dd>
                                    <% else %>
                                        <dd class="mb-1"><span class="label label-danger">InActive</span></dd>
                                    <% end %>
                                </div>
                            </dl>
                        </div>

                        <!-- Name -->
                        <div class="col-lg-6">
                            <dl class="row mb-0">
                                <div class="col-sm-4 text-sm-right">
                                    <dt>Name:</dt>
                                </div>
                                <div class="col-sm-8 text-sm-left">
                                    <dd class="mb-1"><%= @organization.name%></dd>
                                </div>
                            </dl>
                        </div>

                        <!-- Inactive Date -->
                        <div class="col-lg-6">
                            <dl class="row mb-0">
                                <div class="col-sm-4 text-sm-right">
                                    <dt>Inactive Date:</dt>
                                </div>
                                <div class="col-sm-8 text-sm-left">
                                    <% if @organization.inactive_date %>
                                        <dd class="mb-1"><%= @organization.inactive_date.in_time_zone(@organization.timezone).strftime("%b %e, %Y %l:%m %P") %></dd>
                                    <% else %>
                                        <dd class="mb-1">N/A</dd>
                                    <% end %>
                                </div>
                            </dl>
                        </div>

                        <!-- Address -->
                        <div class="col-lg-6">
                            <dl class="row mb-0">
                                <div class="col-sm-4 text-sm-right">
                                    <dt>Address:</dt>
                                </div>
                                <div class="col-sm-8 text-sm-left">
                                    <dd class="mb-1"><%= @organization.street %></dd>
                                    <dd class="mb-1"><%= @organization.street2 %></dd>
                                    <dd class="mb-1"><%= @organization.city %>, <%= @organization.state_providence %> <%= @organization.postal_code %></dd>
                                </div>
                            </dl>
                        </div>

                        <!-- Size -->
                        <div class="col-lg-6">
                            <dl class="row mb-0">
                                <div class="col-sm-4 text-sm-right">
                                    <dt>Org Size:</dt>
                                </div>
                                <div class="col-sm-8 text-sm-left">
                                    <dd class="mb-1"><%= @organization.size %></dd>
                                </div>
                            </dl>
                        </div>

                        <!-- Industry -->
                        <div class="col-lg-6">
                            <dl class="row mb-0">
                                <div class="col-sm-4 text-sm-right">
                                    <dt>Industry:</dt>
                                </div>
                                <div class="col-sm-8 text-sm-left">
                                    <dd class="mb-1"><%= @organization.industry %></dd>
                                </div>
                            </dl>
                        </div>

                        <!-- Time Zone -->
                        <div class="col-lg-6">
                            <dl class="row mb-0">
                                <div class="col-sm-4 text-sm-right">
                                    <dt>Time Zone:</dt>
                                </div>
                                <div class="col-sm-8 text-sm-left">
                                    <dd class="mb-1"><%= @organization.timezone %></dd>
                                </div>
                            </dl>
                        </div>

                        <!-- Total  Contacts -->
                        <div class="col-lg-6">
                            <dl class="row mb-0">
                                <div class="col-sm-4 text-sm-right">
                                    <dt>Contact Count:</dt>
                                </div>
                                <div class="col-sm-8 text-sm-left">
                                    <dd class="mb-1"><%= number_with_delimiter(@organization.contacts.count) %></dd>
                                </div>
                            </dl>
                        </div>

                        <!-- Created At -->
                        <div class="col-lg-6">
                            <dl class="row mb-0">
                                <div class="col-sm-4 text-sm-right">
                                    <dt>Created On:</dt>
                                </div>
                                <div class="col-sm-8 text-sm-left">
                                    <dd class="mb-1"><%= @organization.created_at.in_time_zone(@organization.timezone).strftime("%b %e, %Y %l:%m %P")%></dd>
                                </div>
                            </dl>
                        </div>

                        <!-- Updated At -->
                        <div class="col-lg-6">
                            <dl class="row mb-0">
                                <div class="col-sm-4 text-sm-right">
                                    <dt>Updated On:</dt>
                                </div>
                                <div class="col-sm-8 text-sm-left">
                                    <dd class="mb-1"><%= @organization.updated_at.in_time_zone(@organization.timezone).strftime("%b %e, %Y %l:%m %P")%></dd>
                                </div>
                            </dl>
                        </div>

                    </div>
                    <% if @organization.active %>
                        <hr />
                        <div class="row">
                            <div class="col-lg-12 text-center">
                                <%= link_to "Deactivate Account", admin_page_deactivate_organization_path(organization_id: @organization.id), class: "btn btn-danger btn-md", method: "delete" %>
                            </div>
                        </div>
                    <% end %>
                </div>
            </div>
        </div>
    </div>

    <!-- Number Details -->
    <div class="row" id="org-phone">
        <div class="col-lg-12">
            <div class="ibox">
                <div class="ibox-content">
                    <div class="row">
                        <div class="col-lg-12 text-center">
                            <h3>Phone Number Details</h3>
                            <hr />
                        </div>
                    </div>
                    <div class="row">
                        <!-- Mass Out Going Number -->
                        <div class="col-lg-6">
                            <dl class="row mb-0">
                                <div class="col-sm-4 text-sm-right">
                                    <dt>Mass Out Going:</dt>
                                </div>
                                <div class="col-sm-8 text-sm-left">
                                    <dd class="mb-1">
                                        <% out = @organization.organization_phone_relationships.where(mass_outgoing: true).first %>
                                        <% if !out.nil? %>
                                            <%= out.phone_number.pretty %>
                                        <% else %>
                                            No Number
                                        <% end %>
                                    </dd>
                                </div>
                            </dl>
                        </div>

                        <!-- General Out Going Number -->
                        <div class="col-lg-6">
                            <dl class="row mb-0">
                                <div class="col-sm-4 text-sm-right">
                                    <dt>One To One:</dt>
                                </div>
                                <div class="col-sm-8 text-sm-left">
                                    <dd class="mb-1">
                                        <% one = @organization.organization_phone_relationships.where(mass_outgoing: false).first %>
                                        <% if !one.nil? %>
                                            <%= one.phone_number.pretty %>
                                        <% else %>
                                            No Number
                                        <% end %>
                                    </dd>
                                </div>
                            </dl>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Plan Details -->
    <div class="row" id="org-plan">
        <div class="col-lg-12">
            <div class="ibox">
                <div class="ibox-content">
                    <div class="row">
                        <div class="col-lg-12 text-center">
                            <h3>Plan Details</h3>
                            <small>All times and date are shown in the organization's time zone.</small>
                            <hr />
                        </div>
                    </div>
                    <div class="row">
                        <!-- Plan Name -->
                        <div class="col-lg-6">
                            <dl class="row mb-0">
                                <div class="col-sm-4 text-sm-right">
                                    <dt>Plan:</dt>
                                </div>
                                <div class="col-sm-8 text-sm-left">
                                    <dd class="mb-1"><%= @organization.plan.name%></dd>
                                </div>
                            </dl>
                        </div>

                        <!-- Outside Sale -->
                        <div class="col-lg-6">
                            <dl class="row mb-0">
                                <div class="col-sm-4 text-sm-right">
                                    <dt>Outside Sale:</dt>
                                </div>
                                <div class="col-sm-8 text-sm-left">
                                    <% if @organization.outside_sale %>
                                        <dd class="mb-1"><span class="label label-primary">Yes</span></dd>
                                    <% else %>
                                        <dd class="mb-1"><span class="label label-danger">No</span></dd>
                                    <% end %>
                                </div>
                            </dl>
                        </div>

                        <!-- Plan Credits -->
                        <div class="col-lg-6">
                            <dl class="row mb-0">
                                <div class="col-sm-4 text-sm-right">
                                    <dt>Plan Credits:</dt>
                                </div>
                                <div class="col-sm-8 text-sm-left">
                                    <dd class="mb-1"><%=number_with_delimiter(@organization.annual_credits) %></dd>
                                </div>
                            </dl>
                        </div>

                        <!-- Plan Used Credits -->
                        <div class="col-lg-6">
                            <dl class="row mb-0">
                                <div class="col-sm-4 text-sm-right">
                                    <dt>Used Credits:</dt>
                                </div>
                                <div class="col-sm-8 text-sm-left">
                                    <dd class="mb-1"><%=number_with_delimiter(@organization.current_credit_usage) %></dd>
                                </div>
                            </dl>
                        </div>

                        <!-- Plan Keyword -->
                        <div class="col-lg-6">
                            <dl class="row mb-0">
                                <div class="col-sm-4 text-sm-right">
                                    <dt>Plan Keywords:</dt>
                                </div>
                                <div class="col-sm-8 text-sm-left">
                                    <dd class="mb-1"><%=number_with_delimiter(@organization.plan.keywords_included) %></dd>
                                </div>
                            </dl>
                        </div>

                        <!-- Plan Keyword Usage -->
                        <div class="col-lg-6">
                            <dl class="row mb-0">
                                <div class="col-sm-4 text-sm-right">
                                    <dt>Used Keywords:</dt>
                                </div>
                                <div class="col-sm-8 text-sm-left">
                                    <dd class="mb-1"><%=number_with_delimiter(@organization.keywords.count) %></dd>
                                </div>
                            </dl>
                        </div>

                        <!-- Plan Start Date -->
                        <div class="col-lg-6">
                            <dl class="row mb-0">
                                <div class="col-sm-4 text-sm-right">
                                    <dt>Plan Start Date:</dt>
                                </div>
                                <div class="col-sm-8 text-sm-left">
                                    <% if @organization.plan_start_date %>
                                        <dd class="mb-1"><%= @organization.plan_start_date.in_time_zone(@organization.timezone).strftime("%b %e, %Y %l:%m %P") %></dd>
                                    <% else %>
                                        <dd class="mb-1">N/A</dd>
                                    <% end %>
                                </div>
                            </dl>
                        </div>

                        <!-- Downgrade Date -->
                        <div class="col-lg-6">
                            <dl class="row mb-0">
                                <div class="col-sm-4 text-sm-right">
                                    <dt>Plan Downgrade Date:</dt>
                                </div>
                                <div class="col-sm-8 text-sm-left">
                                    <% if @organization.downgrade_date %>
                                        <dd class="mb-1"><%= @organization.downgrade_date.in_time_zone(@organization.timezone).strftime("%b %e, %Y %l:%m %P") %></dd>
                                    <% else %>
                                        <dd class="mb-1">N/A</dd>
                                    <% end %>
                                </div>
                            </dl>
                        </div>

                        <!-- Outside Sale Rep-->
                        <div class="col-lg-6">
                            <dl class="row mb-0">
                                <div class="col-sm-4 text-sm-right">
                                    <dt>Rep Sale Agent:</dt>
                                </div>
                                <div class="col-sm-8 text-sm-left">
                                    <dd class="mb-1">N/A</dd>
                                </div>
                            </dl>
                        </div>

                    </div>
                    <!-- Plan Actions -->
                    <hr />
                    <div class="row justify-content-md-center">
                        <div class="col-lg-12 text-center">
                            <h3>Plan Actions</h3>
                        </div>
                        <div class="col-lg-4 text-center">
                            <%= link_to "Change Their Plan", admin_page_change_organization_plan_path(organization_id: @organization.id), class: "btn btn-block btn-primary"%>
                        </div>
                    </div>
                    <hr />
                    <div class="row justify-content-md-center">
                        <h3>Plan History</h3>
                    </div>
                    <hr />
                    <% for plan_history in @organization.organization_plan_relationships.order("plan_end_date DESC").limit(5) %>
                        <div class="row">
                            <!-- Plan Name -->
                            <div class="col-lg-6">
                                <dl class="row mb-0">
                                    <div class="col-sm-4 text-sm-right">
                                        <dt>Plan:</dt>
                                    </div>
                                    <div class="col-sm-8 text-sm-left">
                                        <dd class="mb-1"><%= plan_history.plan.name%></dd>
                                    </div>
                                </dl>
                            </div>

                            <!-- Plan Status -->
                            <div class="col-lg-6">
                                <dl class="row mb-0">
                                    <div class="col-sm-4 text-sm-right">
                                        <dt>Status:</dt>
                                    </div>
                                    <div class="col-sm-8 text-sm-left">
                                        <% if plan_history.active %>
                                            <dd class="mb-1"><span class="label label-primary">Active</span></dd>
                                        <% else %>
                                            <dd class="mb-1"><span class="label label-danger">Inactive</span></dd>
                                        <% end %>
                                    </div>
                                </dl>
                            </div>

                            <!-- Plan Start Date -->
                            <div class="col-lg-6">
                                <dl class="row mb-0">
                                    <div class="col-sm-4 text-sm-right">
                                        <dt>Plan Start Date:</dt>
                                    </div>
                                    <div class="col-sm-8 text-sm-left">
                                        <dd class="mb-1"><%= plan_history.plan_start_date.nil? ? "N/A" : plan_history.plan_start_date.in_time_zone(@organization.timezone).strftime("%b %e, %Y %l:%m %P") %></dd>
                                    </div>
                                </dl>
                            </div>

                            <!-- Plan End Date -->
                            <div class="col-lg-6">
                                <dl class="row mb-0">
                                    <div class="col-sm-4 text-sm-right">
                                        <dt>Plan End Date:</dt>
                                    </div>
                                    <div class="col-sm-8 text-sm-left">
                                        <dd class="mb-1"><%= plan_history.plan_end_date.nil? ? "N/A" : plan_history.plan_end_date.in_time_zone(@organization.timezone).strftime("%b %e, %Y %l:%m %P") %></dd>
                                    </div>
                                </dl>
                            </div>

                            <!-- Plan Canceled Date -->
                            <div class="col-lg-6">
                                <dl class="row mb-0">
                                    <div class="col-sm-4 text-sm-right">
                                        <dt>Plan Canceled Initiated:</dt>
                                    </div>
                                    <div class="col-sm-8 text-sm-left">
                                        <dd class="mb-1"><%= plan_history.plan_canceled_date.nil? ? "N/A" : plan_history.plan_canceled_date.in_time_zone(@organization.timezone).strftime("%b %e, %Y %l:%m %P") %></dd>
                                    </div>
                                </dl>
                            </div>

                            <!-- Plan Type -->
                            <div class="col-lg-6">
                                <dl class="row mb-0">
                                    <div class="col-sm-4 text-sm-right">
                                        <dt>Type:</dt>
                                    </div>
                                    <div class="col-sm-8 text-sm-left">
                                        <dd class="mb-1"><%= plan_history.monthly? ? "Monthly" : "Annual" %></dd>
                                    </div>
                                </dl>
                            </div>

                        </div>
                    <% end %>
                </div>
            </div>
        </div>
    </div>

    <!-- Billing Details -->
    <div class="row" id="org-billing">
        <div class="col-lg-12">
            <div class="ibox">
                <div class="ibox-content">
                    <div class="row">
                        <div class="col-lg-12 text-center">
                            <h3>Billing Details</h3>
                            <small>All times and date are shown in the organization's time zone.</small>
                            <hr />
                        </div>
                    </div>
                    <!-- Billing Info -->
                    <div class="row">
                        <% if @organization.stripe_account %>

                            <!-- Payment Card Name -->
                            <div class="col-lg-6">
                                <dl class="row mb-0">
                                    <div class="col-sm-4 text-sm-right">
                                        <dt>Card Holder:</dt>
                                    </div>
                                    <div class="col-sm-8 text-sm-left">
                                        <dd class="mb-1"><%= @organization.stripe_account.payment_source_name%></dd>
                                    </div>
                                </dl>
                            </div>

                            <!-- Payment Card Type -->
                            <div class="col-lg-6">
                                <dl class="row mb-0">
                                    <div class="col-sm-4 text-sm-right">
                                        <dt>Card Type:</dt>
                                    </div>
                                    <div class="col-sm-8 text-sm-left">
                                        <dd class="mb-1"><%= @organization.stripe_account.payment_source_type%></dd>
                                    </div>
                                </dl>
                            </div>

                            <!-- Payment Card Id -->
                            <div class="col-lg-6">
                                <dl class="row mb-0">
                                    <div class="col-sm-4 text-sm-right">
                                        <dt>Card ID:</dt>
                                    </div>
                                    <div class="col-sm-8 text-sm-left">
                                        <dd class="mb-1"><%= @organization.stripe_account.payment_source_id%></dd>
                                    </div>
                                </dl>
                            </div>

                            <!-- Payment Card Last 4 Digits -->
                            <div class="col-lg-6">
                                <dl class="row mb-0">
                                    <div class="col-sm-4 text-sm-right">
                                        <dt>Card Last 4 Digits:</dt>
                                    </div>
                                    <div class="col-sm-8 text-sm-left">
                                        <dd class="mb-1"><%= @organization.stripe_account.payment_source_last4%></dd>
                                    </div>
                                </dl>
                            </div>

                            <!-- Payment Card Exp Month -->
                            <div class="col-lg-6">
                                <dl class="row mb-0">
                                    <div class="col-sm-4 text-sm-right">
                                        <dt>Card Exp Month:</dt>
                                    </div>
                                    <div class="col-sm-8 text-sm-left">
                                        <dd class="mb-1"><%= @organization.stripe_account.payment_source_exp_month%></dd>
                                    </div>
                                </dl>
                            </div>

                            <!-- Payment Card Exp Year -->
                            <div class="col-lg-6">
                                <dl class="row mb-0">
                                    <div class="col-sm-4 text-sm-right">
                                        <dt>Card Exp Year:</dt>
                                    </div>
                                    <div class="col-sm-8 text-sm-left">
                                        <dd class="mb-1"><%= @organization.stripe_account.payment_source_exp_year%></dd>
                                    </div>
                                </dl>
                            </div>

                            <!-- Card Created At -->
                            <div class="col-lg-6">
                                <dl class="row mb-0">
                                    <div class="col-sm-4 text-sm-right">
                                        <dt>Card Created At:</dt>
                                    </div>
                                    <div class="col-sm-8 text-sm-left">
                                        <% if @organization.stripe_account.created_at %>
                                            <dd class="mb-1"><%= @organization.stripe_account.created_at.in_time_zone(@organization.timezone).strftime("%b %e, %Y %l:%m %P") %></dd>
                                        <% else %>
                                            <dd class="mb-1">N/A</dd>
                                        <% end %>
                                    </div>
                                </dl>
                            </div>

                            <!-- Card Updated At -->
                            <div class="col-lg-6">
                                <dl class="row mb-0">
                                    <div class="col-sm-4 text-sm-right">
                                        <dt>Card Updated At:</dt>
                                    </div>
                                    <div class="col-sm-8 text-sm-left">
                                        <% if @organization.stripe_account.created_at %>
                                            <dd class="mb-1"><%= @organization.stripe_account.created_at.in_time_zone(@organization.timezone).strftime("%b %e, %Y %l:%m %P") %></dd>
                                        <% else %>
                                            <dd class="mb-1">N/A</dd>
                                        <% end %>
                                    </div>
                                </dl>
                            </div>

                            <!-- Stripe Id -->
                            <div class="col-lg-6">
                                <dl class="row mb-0">
                                    <div class="col-sm-4 text-sm-right">
                                        <dt>Stripe Id:</dt>
                                    </div>
                                    <div class="col-sm-8 text-sm-left">
                                        <dd class="mb-1"><%= link_to  @organization.stripe_account.stripe_id, "https://dashboard.stripe.com/customers/#{@organization.stripe_account.stripe_id}", target: "_blank" %></dd>
                                    </div>
                                </dl>
                            </div>
                            <br />
                            <div class="col-lg-12 text-center">
                                <%= link_to "Create Invoice",  "https://dashboard.stripe.com/invoices/create?customer=#{@organization.stripe_account.stripe_id}", class: "btn btn-primary", target: "_blank" %>
                            </div>

                        <% else %>
                            <div class="col-lg-12 text-center">
                                <h3>No Billing Details (Please Collect!)</h3>
                            </div>
                        <% end %>
                    </div>
                    <hr />
                    <!-- Card Collection -->
                    <%= form_tag(controller: "admin_page", action: "update_billing" , method: "post") do %>

                        <%= hidden_field_tag :organization_id, @organization.id %>
                        <div class="row">
                            <div class="col-12">
                                <div class="form-group">
                                    <label>NAME AS SHOWN ON CARD</label>
                                    <%= text_field_tag("nameCard", params[:nameCard].nil? ? nil : params[:nameCard], placeholder: "Cardholder Name", required: true, class: "form-control") %>
                                </div>
